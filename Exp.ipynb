    "import math\n",
    "\n",
    "x=int(input())\n",
    "def exp_1(x):\n",
    "    #вычисление ф-ции e^x с помощью ряда Тейлора\n",
    "    n=20\n",
    "    s=1\n",
    "    a=1 #числитель\n",
    "    f=1 #знаменатель\n",
    "    for i in range(1, n):\n",
    "        a*=x\n",
    "        f*=i\n",
    "        s+=a/f\n",
    "    return s\n",
    "print(exp_1(x))"
