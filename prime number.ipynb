    "def prime(a):\n",
    "    p=1\n",
    "    q=0\n",
    "    while p<=a**0.5:\n",
    "        p+=1\n",
    "        if a%p==0: q=1\n",
    "    if q==1:return(0)\n",
    "    else: return(1)"
   ]
