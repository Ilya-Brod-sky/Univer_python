    "def numsys(num: 'int', base: 'int') -> 'str':\n",
    "    a = num\n",
    "    b = base\n",
    "    x = \"\"\n",
    "    \n",
    "    while a > 0:\n",
    "        c = int(48+a%b)\n",
    "        if a % b > 9: c+=7\n",
    "        x = chr(c) + x\n",
    "        a = a//b\n",
    "    return(x)       \n",
    " "
   ]
